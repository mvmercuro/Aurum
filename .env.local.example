# Database
DATABASE_URL=postgresql://user:password@host:5432/database

# OAuth Configuration
NEXT_PUBLIC_APP_ID=your-app-id
NEXT_PUBLIC_OAUTH_PORTAL_URL=https://your-oauth-portal-url
OAUTH_SERVER_URL=https://your-oauth-server-url
OWNER_OPEN_ID=admin-user-openid

# Security
JWT_SECRET=your-jwt-secret

# Forge API (S3 Storage)
BUILT_IN_FORGE_API_URL=https://your-forge-api-url
BUILT_IN_FORGE_API_KEY=your-forge-api-key

# Frontend Forge API (Maps, etc.)
NEXT_PUBLIC_FRONTEND_FORGE_API_KEY=your-frontend-api-key
NEXT_PUBLIC_FRONTEND_FORGE_API_URL=https://your-frontend-forge-url
